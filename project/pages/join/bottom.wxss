/* pages/template/topnav.wxss */
.bottom{
  position: fixed;
  bottom:0;
  left:0;
  width:100%;
  height:120rpx;
  display:flex;
  justify-content: space-between;
  align-items: center;
  font-size:14px;
  text-align: center;
}
.left{
  flex:2;
  display:flex;
  justify-content: space-around;
}
.call,.come,.shop{
  font-size:12px;
}
.icon{
  width:60rpx;
  height:60rpx;
}
.right{
display:flex;
}
.add,.buy{
  display: block;
  width:200rpx;
  height:120rpx;
  text-align: center;
  line-height:120rpx;
  color:#fff;
}
.add{
  background:#FFCF36;
}
.buy{
  background:#F63009;
}